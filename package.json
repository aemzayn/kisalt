{
  "name": "kisalt",
  "description": "URL Shortener app",
  "private": true,
  "author": "Ahmad Muslih Zain",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check-types": "tsc --pretty --noEmit",
